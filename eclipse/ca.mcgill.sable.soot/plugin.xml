      <editor
            name="Jimple Editor"
            icon="icons/soot.png"
            extensions="jimple,grimp,jimp,grimple,shimple,shimp,baf"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
