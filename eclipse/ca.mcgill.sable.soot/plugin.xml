   <extension point="org.eclipse.ui.markerImageProviders">
    <imageprovider id="ca.mcgill.sable.soot.sootAttributeProvider"
        markertype="ca.mcgill.sable.soot.sootattributemarker"
        icon="icons/soot_attribute.png">
    </imageprovider>
   </extension>
   
   <extension
         id="sootattributemarker"
         name="Soot Attribute Marker"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="false">
      </persistent>
      <super
            type="org.eclipse.core.resources.bookmark">
      </super>
      <super
            type="org.eclipse.core.resources.taskmarker">
      </super>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
   </extension>
   
   <extension point="org.eclipse.ui.markerResolution"> 
   	<markerResolutionGenerator 
      class="ca.mcgill.sable.soot.attributes.SootAttributeResolutionGenerator" 
      markerType="ca.mcgill.sable.soot.sootattributemarker"> 
       
   </markerResolutionGenerator> 
   
   </extension> 

   
   <extension
         point="org.eclipse.jdt.ui.javaEditorTextHovers">
      <hover
            class="ca.mcgill.sable.soot.attributes.SootAttributesJavaHover"
            id="ca.mcgill.sable.soot.attributes.SootAttributesJavaHover">
      </hover>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Jimple Editor"
