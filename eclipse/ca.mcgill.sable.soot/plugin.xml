            extensions="jimple,grimp"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            class="ca.mcgill.sable.soot.editors.JimpleEditor"
            id="ca.mcgill.sable.soot.editors.JimpleEditor">
